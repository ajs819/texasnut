{% extends "base.twig" %}

{% block content %}
<div class="content-wrapper">
  <article class="post-type-{{post.post_type}}" id="post-{{post.ID}}">
    {% for layout in post.meta('layouts') %}
      {% include 'layouts/' ~ layout.acf_fc_layout ~ '.twig' %}
    {% endfor %}

    <div class="bg-dark" style="height: 1000px;"></div>
  </article>
</div><!-- /content-wrapper -->
{% endblock %}